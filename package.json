{
  "name": "star-manager",
  "version": "0.0.1",
  "description": "A tool for Github star management.",
  "main": "main.js",
  "dependencies": {
    "electron": "^2.0.8"
  },
  "devDependencies": {
    "concurrently": "^4.0.1",
    "cross-env": "^5.2.0",
    "parcel-bundler": "^1.9.7"
  },
  "scripts": {
    "start": "concurrently --kill-others \"npm:dev\" \"npm:electron:dev\"",
    "electron:dev": "cross-env HOT=1 NODE_ENV=development electron .",
    "dev": "parcel --target electron ./src/renderer.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomoyuen/star-manager.git"
  },
  "keywords": [
    "Github",
    "Star",
    "Electron",
    "React"
  ],
  "author": "tomoyuen",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/tomoyuen/star-manager/issues"
  },
  "homepage": "https://github.com/tomoyuen/star-manager#readme"
}
