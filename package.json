{
  "name": "star-manager",
  "version": "0.0.1",
  "description": "A tool for Github star management.",
  "main": "main.js",
  "dependencies": {
    "electron": "^2.0.8",
    "mobx": "^5.1.0",
    "mobx-react": "^5.2.5",
    "prop-types": "^15.6.2",
    "react": "^16.4.2",
    "react-dom": "^16.4.2"
  },
  "devDependencies": {
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "concurrently": "^4.0.1",
    "cross-env": "^5.2.0",
    "electron-react-devtools": "^0.5.3",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "parcel-bundler": "^1.9.7",
    "parcel-plugin-eslint": "^1.0.4",
    "postcss-cssnext": "^3.1.0",
    "postcss-modules": "^1.3.2",
    "postcss-nested": "^3.0.0",
    "postcss-reporter": "^6.0.0",
    "postcss-utils": "^1.0.2",
    "stylelint": "^9.5.0",
    "stylelint-config-standard": "^18.2.0"
  },
  "scripts": {
    "start": "concurrently --kill-others \"npm:dev\" \"npm:electron:dev\"",
    "debug": "concurrently --kill-others \"npm:dev\" \"npm:electron:debug\"",
    "dev": "parcel --target electron --public-url ./ ./src/index.html",
    "electron:dev": "cross-env NODE_ENV=development electron .",
    "electron:debug": "cross-env NODE_ENV=development debug=1 electron ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomoyuen/star-manager.git"
  },
  "keywords": [
    "Github",
    "Star",
    "Electron",
    "React"
  ],
  "author": "tomoyuen",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/tomoyuen/star-manager/issues"
  },
  "homepage": "https://github.com/tomoyuen/star-manager#readme"
}
